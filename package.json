{
  "name": "@apptweak/cy-picklejs",
  "version": "1.0.1",
  "main": "buildLib",
  "author": "DakaD",
  "description": "Cucumber with Brine",
  "keywords": [
    "js",
    "cypress",
    "pickle",
    "regex"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dakad/cy-picklejs.git"
  },
  "homepage": "https://github.com/Dakad/cy-picklejs#readme",
  "bugs": {
    "url": "https://github.com/Dakad/cy-picklejs/issues"
  },
  "dependencies": {
    "hex-rgb": "^3.0.0",
    "pluralize": "^7.0.0",
    "words-to-numbers": "^1.5.1"
  },
  "peerDependencies": {
    "cypress": "^7.3.0",
    "cypress-cucumber-preprocessor": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.2.6",
    "chai": "^4.2.0",
    "jest": "^25.2.7",
    "jest-cucumber": "^1.0.11"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/common/**/*.js",
      "**/cypress/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageDirectory": "coverage",
    "testMatch": [
      "<rootDir>/**/*.spec.js"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/*"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage/",
      "/dist/",
      "/bin/"
    ]
  }
}
